<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.recyclerviewexample">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="FitNUS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecyclerViewExample"
        tools:targetApi="31">
        <activity
            android:name=".StartExercise"
            android:exported="false" />
        <activity
            android:name=".PrepExercise"
            android:exported="false"
            android:parentActivityName=".FinalPage"/>
        <activity
            android:name=".WorkoutHistoryInfoActivity"
            android:exported="false"
            android:parentActivityName=".MainPage" />
        <activity
            android:name=".FinalPage"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".InsertExercise"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainPage"
            android:exported="false" />
        <activity
            android:name=".Registration"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:parentActivityName=".MainPage" />
        <activity
            android:name=".DetailExerciseActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>